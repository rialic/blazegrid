{
    "private": true,
    "packageManager": "yarn@3.2.2",
    "scripts": {
        "dev": "yarn development",
        "development": "mix",
        "watch": "mix watch",
        "wat": "yarn watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "yarn production",
        "production": "mix --production",
        "yarn-enable": "corepack enable && yarn set version stable",
        "yarn-enable-vscode": "yarn dlx @yarnpkg/sdks vscode",
        "yarn-destroy": "rm -Rf .yarn && rm -Rf .vscode && rm -Rf .editorconfig && rm -Rf .pnp.cjs && rm -Rf .pnp.loader.mjs && rm -f .yarnrc.yml && rm -f yarn.lock && rm -Rf node_modules"
    },
    "devDependencies": {
        "@babel/core": "^7.17.9",
        "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.17.0",
        "@babel/preset-env": "^7.16.11",
        "@babel/runtime": "^7.17.9",
        "@tailwindcss/forms": "^0.4.0",
        "alpinejs": "^3.4.2",
        "autoprefixer": "^10.4.2",
        "cssnano": "^5.1.7",
        "eslint": "^8.13.0",
        "eslint-config-eslintx": "^1.0.4",
        "laravel-mix": "^6.0.43",
        "laravel-mix-purgecss": "^6.0.0",
        "postcss": "^8.4.6",
        "postcss-import": "^14.0.2",
        "postcss-purgecss-laravel": "^2.0.0",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.50.1",
        "sass-loader": "^12.6.0",
        "tailwindcss": "^3.0.18",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.1.2",
        "@popperjs/core": "^2.11.5",
        "bootstrap": "^5.2.0",
        "date-fns": "^2.28.0",
        "date-fns-tz": "^1.3.4"
    }
}
